{
    "family": "task-def-fe",
    "containerDefinitions": [
        {
            "name": "caas-fe",
            "image": "docker.io/ricenzca/caas-fe:main",
            "portMappings": [
                {
                    "name": "http-port",
                    "containerPort": 8080,
                    "hostPort": 8080,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/task-def-fe",
                    "awslogs-create-group": "true",
                    "awslogs-region": "ap-southeast-1",
                    "awslogs-stream-prefix": "ecs"
                },
                "secretOptions": []
            },
            "healthCheck": {
                "command": [
                    "/bin/sh",
                    "-c",
                    "wget --quiet --tries=1 --timeout=5 --spider http://host.docker.internal:8080/api/health || exit 1"
                ],
                "interval": 30,
                "timeout": 5,
                "retries": 3,
                "startPeriod": 30
            }
        }
    ], 
    "taskRoleArn": "arn:aws:iam::211186747480:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::211186747480:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1024",
    "memory": "2048",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
    "registeredAt": "2026-01-21T18:53:03.923Z",
    "registeredBy": "arn:aws:iam::211186747480:user/terence",
    "tags": [
        {
            "key": "project",
            "value": "caas-fe"
        }
    ]
}